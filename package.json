{
  "name": "mathia-backend",
  "version": "1.0.0",
  "description": "Backend API pour Mathia - Application de révision de mathématiques pour collégiens",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:sync": "node src/config/syncDb.js",
    "generate:exercises": "python scripts/ai_generate_exercises.py --chapter \"Les fractions\" --grade \"6ème\" --difficulty \"facile\" --type \"qcm\" --count 10",
    "import:exercises": "python scripts/import_exercises.py --file backend/data/generated_exercises.json --course-id 1",
    "test:api": "node tests/test_api.js",
    "test:api:bash": "bash tests/test_api.sh"
  },
  "keywords": [
    "mathia",
    "education",
    "mathematics",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

